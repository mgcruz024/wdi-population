  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "ca6a3a86",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Function that creates df's for SQL queries  \n",
    "\n",
    "def query_df(query,column_names):\n",
    "    cur.execute(query)\n",
    "    return pd.DataFrame(data=cur.fetchall(),columns=column_names)\n"
   ]
